<section id="torch-toolbar">
  <div class="torch-container">
    <%= link "Cancel", to: Routes.admin_grid_path(@conn, :show, @grid), class: "torch-button" %>
  </div>
</section>
<section id="torch-header-and-content">
  <div class="torch-container">
    <div class="header">
      <h3>Add package to grid</h3>
    </div>
    <%= render "package_form.html", Map.put(assigns, :action, Routes.admin_grid_add_package_path(@conn, :save_package, @grid)) %>
  </div>
</section>
