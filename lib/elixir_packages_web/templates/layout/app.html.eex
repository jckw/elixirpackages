               <defs>
                  <radialGradient id="paint0_radial" cx="0" cy="0" r="1" gradientUnits="userSpaceOnUse" gradientTransform="translate(0.999999 0.5) rotate(46.2454) scale(32.5346)">
                     <stop stop-color="#7C3AED"/>
                     <stop offset="1" stop-color="#A87CF2"/>
